{
  "name": "titanium-backbone",
  "version": "0.5.2",
  "homepage": "https://github.com/trabian/titanium-backbone",
  "description": "Framework for Titanium apps built with Backbone",
  "publishConfig": {
    "registry": "http://npm.trabian.iriscouch.com"
  },
  "dependencies": {
    "backbone": "0.9.x",
    "commander": "0.5.x",
    "jade": "0.20.x",
    "pretty-data": "0.20.x",
    "underscore": "1.3.x",
    "node-uuid": "1.3.x",
    "stitch-up": "*",
    "wrench": "1.3.x"
  },
  "bin": {
    "titanium-backbone": "./bin/titanium-backbone"
  },
  "author": {
    "name": "Matt Dean",
    "email": "matt@trabian.com"
  },
  "bugs": {
    "url": "https://github.com/trabian/titanium-backbone/issues",
    "email": "matt@trabian.com"
  },
  "scripts": {
    "preinstall": "cd node_modules/stitch-up && npm install"
  },
  "main": "./lib/index.coffee",
  "stitch": {
    "paths": [
      "src"
    ],
    "dependencies": [
      "deps/console.coffee",
      "jade:runtime.js"
    ],
    "images": "vendor/images"
  }
}
